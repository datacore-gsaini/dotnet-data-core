        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <system.serviceModel>
    <client>
      <endpoint name="onmachinecommunicationendpoint"
                    contract="CalculatorServiceContractLib.ICalculatorService"
