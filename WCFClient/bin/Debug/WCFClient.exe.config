      <endpoint name="lanclientcommunicationendpoint"
                contract="CalculatorServiceContractLib.ICalculatorService"
                binding="netTcpBinding"
                address="net.tcp://localhost:9000/lanep"></endpoint>
    </client>
  </system.serviceModel>
</configuration>